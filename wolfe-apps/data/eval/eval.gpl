# Chart settings
#set title "Injecting Logic into Synthetic Matrices"
set terminal dumb enhanced
#set term x11
#set termoption enhanced
set key right bottom

if (!exists("fileName")) fileName='experiments'

set yrange [0.0:1.0]
set ytics 0.1,0.1,1

set xlabel "Fraction of Training Data"
set ylabel "MAP/wMAP"

dataMF = "< grep \"None\" ".fileName.".log"
dataMFLogic = "< grep \"formulae\" ".fileName.".log"

plot dataMF using 1:3 smooth unique linecolor rgb "blue" linetype 1 linewidth 3  title "MF",\
     dataMFLogic using 1:3 smooth unique linecolor rgb "red" linetype 1 linewidth 3  title "MF+logic"

# Wrapup
#set terminal pdf enhanced dashed size 3,3#1.75 #size 400,400
set terminal png truecolor size 400,400
set output fileName.".png"
#set output fileName.".pdf"
refresh
unset output